profile mobile {
	output eDP-1 enable
	exec /usr/bin/makoctl mode -r docked-dp
}

profile home {
	output eDP-1 disable
	output 'Acer Technologies XV273K 0x91709387' enable mode 3840x2160 position 1280,0 scale 2
	output 'NEC Corporation LCD195NX 94T19400NB' enable mode 1280x1024@75Hz position 3200,0
	exec /usr/bin/makoctl mode -a docked-dp
}
